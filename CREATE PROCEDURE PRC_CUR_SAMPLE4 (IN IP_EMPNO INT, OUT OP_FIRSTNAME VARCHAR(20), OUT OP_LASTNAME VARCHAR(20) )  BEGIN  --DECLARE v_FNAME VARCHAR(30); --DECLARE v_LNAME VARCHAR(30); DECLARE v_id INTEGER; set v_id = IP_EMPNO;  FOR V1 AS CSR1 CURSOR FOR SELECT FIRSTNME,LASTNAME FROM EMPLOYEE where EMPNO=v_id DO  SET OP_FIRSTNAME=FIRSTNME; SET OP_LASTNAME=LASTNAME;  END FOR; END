CREATE PROCEDURE PRC_CUR_SAMPLE4 (IN IP_EMPNO INT, OUT OP_FIRSTNAME VARCHAR(20), OUT OP_LASTNAME VARCHAR(20) )

BEGIN

--DECLARE v_FNAME VARCHAR(30);
--DECLARE v_LNAME VARCHAR(30);
DECLARE v_id INTEGER;
set v_id = IP_EMPNO;

FOR V1 AS
CSR1 CURSOR FOR SELECT FIRSTNME,LASTNAME FROM EMPLOYEE where EMPNO=v_id
DO

SET OP_FIRSTNAME=FIRSTNME;
SET OP_LASTNAME=LASTNAME;

END FOR;
END
